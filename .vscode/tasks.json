{
  "options": {
    "env": {
      //"Path": "F:\\C++\\WorkSpace\\mingw64\\bin;${env:Path}"
      "Path": "${workspaceFolder}\\mingw64\\bin;${env:Path}"
    },
    "shell": {
      "args": [
        "/C"
      ],
      "executable": "C:\\Windows\\System32\\cmd.exe"
    }
  },
  "tasks": [
    {
      "args": [
        "-std=c++23",
        "-g",
        "${file}",
        "-o",
        //"${fileDirname}\\${fileBasenameNoExtension}.exe"
        "${workspaceFolder}\\Debug\\Temp_${fileBasenameNoExtension}.exe"
      ],
      //"command": "F:\\C++\\WorkSpace\\mingw64\\bin\\g++.exe",
      "command": "${workspaceFolder}\\mingw64\\bin\\g++.exe",
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "gcc single file build",
      "presentation": {
        "clear": true,
        "echo": false,
        "focus": false,
        "panel": "shared",
        "reveal": "silent",
        "showReuseMessage": false
      },
      "problemMatcher": "$gcc",
      "type": "process"
    }
  ],
  "version": "2.0.0"
}